{
  "version": 3,
  "name": "frontend",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.html)",
      "dest": "/html/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/javascript/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/components/(.*)",
      "dest": "/components/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/html/index.html"
    }
  ]
}
