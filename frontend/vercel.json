{
  "version": 2,
  "public": true,
  "name": "frontend",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/about-us", "destination": "/about-us.html" },
    { "source": "/auth", "destination": "/auth.html" },
    { "source": "/create-post", "destination": "/create-post.html" },
    { "source": "/notification-center", "destination": "/notification-center.html" },
    { "source": "/posts", "destination": "/posts.html" },
    { "source": "/profile", "destination": "/profile.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
